{
  "expo": {
    "name": "Fit Court",
    "slug": "fit-court",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon192x192.png",
    "notification": {
      "icon": "./assets/icon144x144.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icon500x500.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.team.fitfast"
    }
  }
}
